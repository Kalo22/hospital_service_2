{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1
        },
        "dateOfBirth": {
            "type": "string",
            "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
        },
        "email": {
            "type": "string",
            "format": "email"
        }
    },
    "required": ["name", "dateOfBirth", "email"],
    "additionalProperties": false
}